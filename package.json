{
  "name": "cytoscape-rdf",
  "version": "0.0.2",
  "author": "Vincent Emonet",
  "type": "module",
  "description": "A Web Component to display RDF quads data as cytoscape network",
  "main": "dist/cytoscape-rdf.js",
  "unpkg": "dist/cytoscape-rdf.min.js",
  "exports": {
    ".": {
      "import": "./dist/cytoscape-rdf.es.js",
      "require": "./dist/cytoscape-rdf.umd.js"
    }
  },
  "types": "types/cytoscape-rdf.d.ts",
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "rollup": "tsc && vite build && rollup -c",
    "preview": "vite preview"
  },
  "dependencies": {
    "@microsoft/fast-element": "^1.7.0",
    "cytoscape": "^3.23.0",
    "cytoscape-cose-bilkent": "^4.1.0",
    "cytoscape-popper": "^2.0.0",
    "n3": "^1.16.3"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/cytoscape": "^3.19.9",
    "@types/cytoscape-popper": "^2.0.0",
    "@types/n3": "^1.10.4",
    "rollup": "^3.17.3",
    "rollup-plugin-filesize": "^9.1.2",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.5.4",
    "vite": "^2.8.0",
    "vite-plugin-minify": "^1.5.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vemonet/cytoscape-rdf.git"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/vemonet/cytoscape-rdf/issues"
  },
  "homepage": "https://github.com/vemonet/cytoscape-rdf"
}
